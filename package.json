  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "clean:ngc": "rimraf src/**/*.ngfactory.ts",
    "ngc": "ngc",
    "webpack": "webpack",
    "watch": "webpack -w",
    "build": "npm run ngc && webpack --config webpack.config.prod.js",
    "build:ghpages": "npm run ngc && webpack --config webpack.config.ghpages.js",
    "start": "browser-sync start --server --files dist/*.js --files *.html --port 3000",
    "predeploy": "npm run clean && npm run build:ghpages && mkdir -p dist/dist && mv dist/*.js dist/dist && cp *.html dist",
    "deploy": "gh-pages -d dist"
  },
